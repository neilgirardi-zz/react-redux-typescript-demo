{
  "name": "react-redux-starter-kit",
  "version": "1.0.0",
  "description": "React started kit based on create-react-app",
  "scripts": {
    "start-server": "ts-node-dev --respawn --transpileOnly ./src/backend/index.ts",
    "start-client": "node ./bin/start-frontend",
    "build-client": "node ./bin/build-frontend",
    "start": "concurrently \"npm run start-server\" \"npm run start-client\"",
    "build": "npm run build-client",
    "setup": "node ./bin/setup",
    "tsc": "tsc"
  },
  "author": "Neil Girardi",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.4",
    "nodemon": "^1.18.9",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.3.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
